{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            false
        ],
        "no-string-literal": false,
        "no-namespace": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "object-literal-sort-keys": false
    }
}